; 循环结构测试 / Loop structure test

; For循环测试 - 遍历列表 / For loop test - iterate over list
(for x (list 1 2 3 4 5)
  (print x))  ; 打印 1, 2, 3, 4, 5

; For循环测试 - 遍历整数范围 / For loop test - iterate over integer range
(for i 5
  (print i))  ; 打印 0, 1, 2, 3, 4

; For循环测试 - 累加 / For loop test - sum
(let sum 0
  (for x (list 10 20 30)
    (set! sum (+ sum x)))
  (print "累加结果 / Sum result:" sum)
  sum)  ; 打印并返回 60

; While循环测试 / While loop test
(let count 0
  (while (< count 5)
    (set! count (+ count 1)))
  (print "While循环结果 / While loop result:" count)
  count)  ; 打印并返回 5

; While循环测试 - 计算阶乘 / While loop test - calculate factorial
(let n 5
  (let result 1
    (while (> n 0)
      (set! result (* result n))
      (set! n (- n 1)))
    (print "阶乘结果 / Factorial result:" result)
    result))  ; 打印并返回 120
